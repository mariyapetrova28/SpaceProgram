<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="30.0.1" />
    <PackageReference Include="LinqToCsv" Version="1.5.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Language\output.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>output.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Language\output.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>output.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
